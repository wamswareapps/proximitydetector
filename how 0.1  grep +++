[32m+[m[32m    @Override[m
[32m+[m[32m    public void onRequestPermissionsResult(int requestCode,[m
[32m+[m[32m                                           String permissions[], int[] grantResults) {[m
[32m+[m[32m        switch (requestCode) {[m
[32m+[m[32m            case MY_PERMISSIONS_REQUEST_FINE_LOCATION: {[m
[32m+[m[32m                // If request is cancelled, the result arrays are empty.[m
[32m+[m[32m                if (grantResults.length > 0[m
[32m+[m[32m                        && grantResults[0] == PackageManager.PERMISSION_GRANTED) {[m
[32m+[m[32m                    setupLocationManagement();[m
[32m+[m[32m                } else {[m
[32m+[m
[32m+[m[32m                    // Disable the functionality that depends on this permission.[m
[32m+[m[32m                }[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
