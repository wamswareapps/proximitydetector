[32m+[m[32m    @SuppressWarnings("MissingPermission")[m
[32m+[m[32m    private Location bestLastKnownLocation(float minAccuracy, long minTime) {[m
[32m+[m[32m        Location bestResult = null;[m
[32m+[m[32m        float bestAccuracy = Float.MAX_VALUE;[m
[32m+[m[32m        long bestTime = Long.MIN_VALUE;[m
[32m+[m
[32m+[m[32m        // Get the best most recent location currently available[m
[32m+[m[32m        Location mCurrentLocation = LocationServices.FusedLocationApi.getLastLocation(mGoogleApiClient);[m
[32m+[m
[32m+[m[32m        if (mCurrentLocation != null) {[m
[32m+[m[32m            float accuracy = mCurrentLocation.getAccuracy();[m
[32m+[m[32m            long time = mCurrentLocation.getTime();[m
[32m+[m
[32m+[m[32m            if (accuracy < bestAccuracy) {[m
[32m+[m[32m                bestResult = mCurrentLocation;[m
[32m+[m[32m                bestAccuracy = accuracy;[m
[32m+[m[32m                bestTime = time;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Return best reading or null[m
[32m+[m[32m        if (bestAccuracy > minAccuracy || bestTime < minTime) {[m
[32m+[m[32m            return null;[m
[32m+[m[32m        }[m
[32m+[m[32m        else {[m
[32m+[m[32m            return bestResult;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
