[32m+[m[32m    private void setupLocationManagement() {[m
[32m+[m
[32m+[m[32m        mLocationRequest = new LocationRequest();[m
[32m+[m[32m        mLocationRequest.setInterval(10);[m
[32m+[m[32m        mLocationRequest.setFastestInterval(1);[m
[32m+[m[32m        mLocationRequest.setPriority(LocationRequest.PRIORITY_HIGH_ACCURACY);[m
[32m+[m
[32m+[m[32m        LocationSettingsRequest.Builder builder = new LocationSettingsRequest.Builder()[m
[32m+[m[32m                .addLocationRequest(mLocationRequest);[m
[32m+[m
[32m+[m[32m        PendingResult<LocationSettingsResult> result =[m
[32m+[m[32m                LocationServices.SettingsApi.checkLocationSettings(mGoogleApiClient,[m
[32m+[m[32m                        builder.build());[m
[32m+[m
[32m+[m[32m        result.setResultCallback(new ResultCallback<LocationSettingsResult>() {[m
[32m+[m[32m            @SuppressWarnings("MissingPermission")[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onResult(@NonNull LocationSettingsResult result) {[m
[32m+[m[32m                final Status status = result.getStatus();[m
[32m+[m[32m                final LocationSettingsStates states = result.getLocationSettingsStates();[m
[32m+[m[32m                switch (status.getStatusCode()) {[m
[32m+[m[32m                    case LocationSettingsStatusCodes.SUCCESS:[m
[32m+[m[32m                        // All location settings are satisfied. The client can[m
[32m+[m[32m                        // initialize location requests here.[m
[32m+[m
[32m+[m[32m                        LocationServices.FusedLocationApi.requestLocationUpdates(mGoogleApiClient, mLocationRequest, ProxyAlertActivity.this);[m
[32m+[m
[32m+[m[32m//                        // Get best last location measurement meeting criteria[m
[32m+[m[32m//                        mBestReading = bestLastKnownLocation(MIN_LAST_READ_ACCURACY, FIVE_MIN);[m
[32m+[m[32m//[m
[32m+[m[32m//                        if (null == mBestReading[m
[32m+[m[32m//                                || mBestReading.getAccuracy() > MIN_LAST_READ_ACCURACY[m
[32m+[m[32m//                                || mBestReading.getTime() < System.currentTimeMillis() - TWO_MIN) {[m
[32m+[m[32m//[m
[32m+[m[32m//                            LocationServices.FusedLocationApi.requestLocationUpdates(mGoogleApiClient, mLocationRequest, ProxyAlertActivity.this);[m
[32m+[m[32m//[m
[32m+[m[32m//                            // Schedule a runnable to unregister location listeners[m
[32m+[m[32m//                            Executors.newScheduledThreadPool(1).schedule(new Runnable() {[m
[32m+[m[32m//[m
[32m+[m[32m//                                @Override[m
[32m+[m[32m//                                public void run() {[m
[32m+[m[32m//                                    LocationServices.FusedLocationApi.removeLocationUpdates(mGoogleApiClient, ProxyAlertActivity.this);[m
[32m+[m[32m//                                }[m
[32m+[m[32m//[m
[32m+[m[32m//                            }, ONE_MIN, TimeUnit.MILLISECONDS);[m
[32m+[m[32m//                        }[m
[32m+[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case LocationSettingsStatusCodes.RESOLUTION_REQUIRED:[m
[32m+[m[32m                        // Location settings are not satisfied, but this can be fixed[m
[32m+[m[32m                        // by showing the user a dialog.[m
[32m+[m[32m                        try {[m
[32m+[m[32m                            // Show the dialog by calling startResolutionForResult(),[m
[32m+[m[32m                            // and check the result in onActivityResult().[m
[32m+[m[32m                            status.startResolutionForResult([m
[32m+[m[32m//                                    OuterClass.this,[m
[32m+[m[32m                                    ProxyAlertActivity.this,[m
[32m+[m[32m                                    REQUEST_CHECK_SETTINGS);[m
[32m+[m[32m                        } catch (IntentSender.SendIntentException e) {[m
[32m+[m[32m                            // Ignore the error.[m
[32m+[m[32m                        }[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case LocationSettingsStatusCodes.SETTINGS_CHANGE_UNAVAILABLE:[m
[32m+[m[32m                        // Location settings are not satisfied. However, we have no way[m
[32m+[m[32m                        // to fix the settings so we won't show the dialog.[m
[32m+[m
[32m+[m[32m                        break;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m//        //noinspection MissingPermission[m
[32m+[m[32m//        mLastLocation = LocationServices.FusedLocationApi.getLastLocation(mGoogleApiClient);[m
[32m+[m
[32m+[m[32m//        //noinspection MissingPermission[m
[32m+[m[32m//        LocationServices.FusedLocationApi.requestLocationUpdates([m
[32m+[m[32m//                mGoogleApiClient, mLocationRequest, this);[m
[32m+[m[32m    }[m
