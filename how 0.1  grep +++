[1mdiff --git a/app/src/main/java/com/wamsware/www/proximitydetector/ProximityIntentReceiver.java b/app/src/main/java/com/wamsware/www/proximitydetector/ProximityIntentReceiver.java[m
[1mnew file mode 100644[m
[1mindex 0000000..0b928b3[m
[1m--- /dev/null[m
[1m+++ b/app/src/main/java/com/wamsware/www/proximitydetector/ProximityIntentReceiver.java[m
[36m@@ -0,0 +1,54 @@[m
[32m+[m[32mpackage com.wamsware.www.proximitydetector;[m
[32m+[m
[32m+[m[32mimport android.app.IntentService;[m
[32m+[m[32mimport android.app.Notification;[m
[32m+[m[32mimport android.app.NotificationManager;[m
[32m+[m[32mimport android.app.PendingIntent;[m
[32m+[m[32mimport android.content.BroadcastReceiver;[m
[32m+[m[32mimport android.content.Context;[m
[32m+[m[32mimport android.content.Intent;[m
[32m+[m[32mimport android.graphics.Color;[m
[32m+[m[32mimport android.location.LocationManager;[m
[32m+[m[32mimport android.support.v4.app.NotificationCompat;[m
[32m+[m[32mimport android.util.Log;[m
[32m+[m
[32m+[m[32mpublic class ProximityIntentReceiver extends BroadcastReceiver {[m
[32m+[m
[32m+[m[32m    private static final int NOTIFICATION_ID = 1000;[m
[32m+[m
[32m+[m[32m    static int counter = 0;[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onReceive(Context context, Intent intent) {[m
[32m+[m
[32m+[m[32m        Boolean entering = intent.getBooleanExtra(LocationManager.KEY_PROXIMITY_ENTERING, false);[m
[32m+[m
[32m+[m[32m        String message = "";[m
[32m+[m[32m        if (entering) {[m
[32m+[m[32m            message = "You are moving nearer to your point of interest.";[m
[32m+[m[32m        }[m
[32m+[m[32m        else {[m
[32m+[m[32m            message = "You are away from your point of interest.";[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        counter++;[m
[32m+[m
[32m+[m[32m        NotificationCompat.Builder notifBldr =[m
[32m+[m[32m                new NotificationCompat.Builder(context)[m
[32m+[m[32m                        .setSmallIcon(R.drawable.ic_apply_event)[m
[32m+[m[32m                        .setContentTitle("Proximity Detector")[m
[32m+[m[32m                        .setContentText(message + " " + String.valueOf(counter));[m
[32m+[m
[32m+[m[32m        NotificationManager notificationManager =[m
[32m+[m[32m                (NotificationManager) context.getSystemService(IntentService.NOTIFICATION_SERVICE);[m
[32m+[m
[32m+[m[32m        PendingIntent pendingIntent = PendingIntent.getActivity(context, 0, intent, PendingIntent.FLAG_ONE_SHOT);[m
[32m+[m
[32m+[m[32m        notifBldr.setContentIntent(pendingIntent);[m
[32m+[m
[32m+[m[32m        notificationManager.notify(NOTIFICATION_ID, notifBldr.build());[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m}[m
